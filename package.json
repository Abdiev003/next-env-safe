{
  "name": "next-env-safe",
  "version": "1.0.0",
  "description": "Type-safe, Next.js-ready environment variable manager (server/client separation, Zod validation, build-time fail).",
  "license": "MIT",
  "author": "Ali <aliabdiyev000@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abdiev003/next-env-safe"
  },
  "bugs": {
    "url": "https://github.com/Abdiev003/next-env-safe/issues"
  },
  "keywords": [
    "nextjs",
    "react",
    "environment",
    "env",
    "zod",
    "typesafe",
    "validation"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "zod": ">=3.22.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.2",
    "zod": "^3.23.8"
  }
}
